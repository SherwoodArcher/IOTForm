        $("#voltarProjeto").off().click(function () {
            getContent("/Dashboard/Projeto");
        });
        $(".part1btn").off().click(function () {
            $(".wrapper").hide();
            $("#part1").show();
        });
        $(".part15btn").off().click(function () {
            $(".wrapper").hide();
            $("#part15").show();
        });
        $(".part2btn").off().click(function () {
            $(".wrapper").hide();
            $("#part2").show();
        });
        $(".part3btn").off().click(function () {
            $(".wrapper").hide();
            $("#part3").show();
        });
        $(".part4btn").off().click(function () {
            $(".wrapper").hide();
            $("#part4").show();
        });
        $(".salvar").off().click(function () {
            enviarAtivosDevices();
        });
        loadAtivosDevices();
    });
</script>
